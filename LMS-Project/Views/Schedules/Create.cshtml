@{
    ViewBag.Title = "Create";
}

<h2>Create a new Schedule</h2>

<div class="form-horizontal"
     ng-app="schedule"
     ng-controller="schedulesController"
     ng-init="init()">


    @Html.Partial("_CreateEdit")

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
            <button type="button" class="btn btn-default" ng-click="sendData()">Create</button>
            </div>
        </div>
    </div>

<div>
    @Html.ActionLink("Back to Schedules", "Index")
</div>

<div id="Debug" ng-repeat="Sch"></div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/angularschedules")
}

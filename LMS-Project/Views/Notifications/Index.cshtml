@model IEnumerable<LMS_Project.Models.LMS.Notification>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Grade.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SendingDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReadingDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Grade.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SendingDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReadingDate)
        </td>
    </tr>
}

</table>
